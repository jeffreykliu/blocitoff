<h1>Your List</h1>
</br>
<div class="row">
  <div class="col-md-6">

    <%= form_for @list do |f| %>
      
      <div class="form-group">
        <%= f.label :title %>
        <%= f.text_field :title, autofocus: true, class: 'form-control', placeholder: "Enter to-do list title" %>
      </div>

      <div class="form-group">
        <%= f.submit "Update List", class: 'btn btn-success' %>
      </div>

    <% end %>

  </div>
</div>